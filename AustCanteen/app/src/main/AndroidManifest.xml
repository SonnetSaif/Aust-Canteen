<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.austcanteen">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".BreakFastMenu" />
        <activity android:name=".LunchMenu" />
        <activity android:name=".DrinksMenu" />
        <activity android:name=".CartMenu" />
        <activity android:name=".ComplaintBox" />
        <activity android:name=".OtherItems" />
        <activity android:name=".ConfirmOrder" />
        <activity android:name=".OrderMenu" />
        <activity android:name=".ShowOrderedItems" />
        <activity android:name=".Developers"></activity>
    </application>

</manifest>